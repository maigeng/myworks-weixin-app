page {
  width: 100%;
  height: 100%;
}

.result {
  width: 100%;
  height: 100%;
  background: url("https://cdn.nlark.com/yuque/0/2021/png/164272/1616256978686-1a4bbc2d-7ee1-44e4-ae97-a04ca5239a86.png?x-oss-process=image%2Fresize%2Cw_1362") no-repeat center center;
  background-size: 100% 100%;
  padding: 30rpx 43rpx 26rpx 43rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.result .res {
  width: 100%;
  height: 373rpx;
  display: flex;
  align-items: center;
  padding: 80rpx 56rpx 40rpx 56rpx;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
  background: url("https://cdn.nlark.com/yuque/0/2021/png/12512897/1617798893208-4cf6a050-4928-4c82-ac4f-0b694fc4b353.png") no-repeat center center;
  background-size: 100% 100%;
}

.result .res view {
  width: 230rpx;
  font-family: zihun100;
  font-size: 60rpx;
  margin-top: 40rpx;
  margin-left: 20rpx;
  box-sizing: border-box;
  text-align: center;
  color: #676cf0;
}

.result .res image {
  padding-top: 35rpx;
  width: 160rpx;
  display: block;
  box-sizing: border-box;
}

.result .tag {
  width: 100%;
  margin-bottom: 42rpx;
}

.result .tag h1 {
  width: 100%;
  font-family: AppleColorEmoji;
  font-size: 36rpx;
  color: #F5F5F5;
  letter-spacing: 0.6rpx;
  margin-bottom: 23rpx;
  display: flex;
  align-items: center;
}

.result .tag h1 image {
  width: 40rpx;
  height: 40rpx;
  display: block;
}

.result .tag h1 span {
  color: #FFE68D;
  margin-left: 10rpx;
}

.result .tag .list1 {
  display: flex;
}

.result .tag .list1 p:first-child {
  margin-right: 29rpx;
}

.result .tag .list1 p {
  font-family: PingFangSC-Semibold;
  font-size: 27rpx;
  color: #F5F5F5;
  padding: 16rpx 34rpx;
  box-sizing: border-box;
  background: rgba(0, 118, 255, 0.7);
  box-shadow: 0 1rpx 11rpx 5rpx rgba(255, 255, 255, 0.2);
  border-radius: 0 33rpx 33rpx 33rpx;
}

.result .tag .list2 p {
  display: inline-block;
  font-family: PingFangSC-Semibold;
  font-size: 27rpx;
  color: #F5F5F5;
  padding: 16rpx 34rpx;
  background: rgba(122, 52, 244, 0.7);
  box-shadow: 0 1rpx 11rpx 5rpx rgba(254, 254, 254, 0.13);
  border-radius: 0 44rpx 44rpx 44rpx;
}

.result .tag .list2 p:first-child {
  margin-bottom: 30rpx;
}

.result .title {
  width: 491rpx;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #FFFDE6;
  letter-spacing: 0;
  display: block;
  margin: 30rpx auto 0;
}

.result .btn {
  width: 100%;
  display: flex;
  height: 87rpx;
  margin-top: 30rpx;
  justify-content: space-between;
  align-items: center;
}

.result .btn button {
  width: calc(100%/2 - 16rpx);
  height: 87rpx;
  background: #FFF270;
  box-shadow: 0 4rpx 16rpx 0 rgba(125, 70, 45, 0.48);
  border-radius: 16rpx;
  height: 100%;
  line-height: 87rpx;
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  color: #8184FF;
  letter-spacing: 0;
  text-align: center;
}

.result .btn view {
  width: calc(100%/2 - 16rpx);
  height: 87rpx;
  background: #FFF270;
  box-shadow: 0 4rpx 16rpx 0 rgba(125, 70, 45, 0.48);
  border-radius: 16rpx;
  height: 100%;
  line-height: 87rpx;
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  color: #8184FF;
  letter-spacing: 0;
  text-align: center;
  padding: 0;
}

.result .btn button:first-child {
  margin-right: 32rpx;
}

.result > .tips {
  width: 491rpx;
  margin: 33rpx auto 22rpx;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-size: 18rpx;
  color: #FFFDE6;
  letter-spacing: 0;
  text-align: center;
  display: block;
}

.result .footer {
  width: 100%;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.result .footer image {
  width: 40rpx;
  height: 40rpx;
  display: block;
}

.result .footer span {
  font-family: SignPainter-HouseScript;
  font-size: 24rpx;
  color: #FFFDE6;
  letter-spacing: 0;
  text-align: center;
  margin-left: 13rpx;
}
