page {
  width: 100%;
  height: 100%;
}

.uploadpage {
  width: 100%;
  height: 100%;
  background: url("https://cdn.nlark.com/yuque/0/2021/png/164272/1616256978686-1a4bbc2d-7ee1-44e4-ae97-a04ca5239a86.png?x-oss-process=image%2Fresize%2Cw_1362") no-repeat center center;
  background-size: 100% 100%;
  padding: 30rpx 43rpx 26rpx 43rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.uploadpage > image {
  width: 500rpx;
  height: 400rpx;
  display: block;
  margin: 0 auto;
}

.uploadpage .title {
  width: 100%;
  background: url("https://cdn.nlark.com/yuque/0/2021/png/164272/1616939057039-15cadcad-f520-4f02-8288-883a0bbbac25.png") no-repeat center center;
  background-size: 100% 100%;
  height: 130rpx;
  line-height: 130rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  padding: 14rpx 0 0 50rpx;
  box-sizing: border-box;
  margin-top: -40rpx;
}

.uploadpage .imgList {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-top: 40rpx;
}

.uploadpage .imgList > view {
  width: 33.3%;
  margin: 10rpx 5rpx;
}

.uploadpage .imgList > view image {
  width: 100%;
}

.uploadpage .addimg {
  width: 200rpx;
  height: 200rpx;
  color: #666;
  display: flex;
  border: solid 2rpx #666;
  align-items: center;
  justify-content: center;
  font-size: 80rpx;
  font-weight: bold;
}

.uploadpage .logo {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}

.uploadpage .logo image {
  width: 80rpx;
  height: 80rpx;
  display: flex;
}

.uploadpage .logo p {
  font-family: PingFangSC-Medium;
  font-size: 16rpx;
  color: #ffffff;
}
